{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-var": "error",
    "semi": ["error", "always"],
    "no-prototype-builtins": "off",
    "prefer-const": "error",
    "camelcase": "error"
  }
}
